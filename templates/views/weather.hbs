<!DOCTYPE html>
<html lang="en">
<head>
    {{>header}}
</head>
<body>
{{>navbar}}
     <!-- main  -->
     <div class="container-fluid main_header">
        <div class="row">
            <div class="col-md-10 col-12 mx-auto">
                <div class="main_content">
                    <form class="temp_form">
                        <input type="text" id="cityname" placeholder="Enter name of any City" autocomplete="off">
                        <input type="submit" value="search" id="submitbtn">
                    </form>
                </div>

                <div class="tempinformation">
                    <div class="top_layer">
                        <p id="day">Nice Day!</p>
                      {{!-- <p id="today_data"><a href="https://www.google.com/maps/place/Pune"><button>See Location</button></a></p> --}}
                    </div>
                    <div class="main_layer ">
                        <p id="city_name">Get Output Here</p>
                        <div class="middle_layer data_hide">
                            <p id="temp"><span id="temp_real_val">0</span><sup>o</sup>C</p>
                            <p id="temp_status"> <i class="fa fa-cloud" aria-hidden="true"></i></p>
                        </div>

                    </div>
                </div>
            </div>
        </div>
     </div>
     <!-- footer  -->
     {{>footer}}
     <script src="js/main.js"></script>
</body>
</html>